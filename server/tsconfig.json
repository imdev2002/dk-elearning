{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "allowJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "out/**",
    "dist",
    "dist/**"
  ],
  "include": ["**/*.ts"]
}

